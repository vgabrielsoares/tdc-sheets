    <changeSet id="001-insert-dev-fichas" author="tdc-sheets">
        <comment>Inserção de fichas de exemplo para desenvolvimento usando dados CSV</comment>
        
        <loadData 
            encoding="UTF-8"
            file="db/fake-data/fichas.csv"
            separator=";"
            tableName="ficha_personagem">
            <column name="id" type="NUMERIC"/>
            <column name="nome_personagem" type="STRING"/>
            <column name="user_id" type="NUMERIC"/>
            <column name="background" type="STRING"/>
            <column name="origem_id" type="NUMERIC"/>
            <column name="linhagem_id" type="NUMERIC"/>
            <column name="experiencia" type="NUMERIC"/>
            <column name="habilidade_assinatura" type="NUMERIC"/>
            <column name="nivel_sorte" type="NUMERIC"/>
            <column name="tamanho" type="STRING"/>
            <column name="created_at" type="TIMESTAMP"/>
            <column name="is_active" type="BOOLEAN"/>
        </loadData>
        
        <rollback>
            DELETE FROM ficha_personagem WHERE id IN (1, 2, 3);
        </rollback>
    </changeSet>
    
